  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@testing-library/react-native": "^11.4.0",
    "eslint": "^8.26.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "prettier": "^2.7.1"
  },
  "private": true
